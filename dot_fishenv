# virtualenv python
set WORKON_HOME ~/.virtualenvs
set PIP_VIRTUALENV_BASE ~/.virtualenvs
set PIPENV_VENV_IN_PROJECT True

# gopath go
set -x GOPATH ~/.go

# borg-backup pass
set -x BORG_PASSCOMMAND cat ~/.borg-passphrase

# anime-dl client id for anilist
set -x ANIMEDL_ID 4047

# localstack
set -x EXTRA_CORS_ALLOWED_ORIGINS '*'

# esp toolkit
set -x LIBCLANG_PATH "/home/gabriele/.rustup/toolchains/esp/xtensa-esp32-elf-clang/esp-16.0.4-20231113/esp-clang/lib"
